{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android:release": "yarn android --variant release",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "ios": "npx react-native run-ios",
    "ios:release": "yarn ios --configuration Release",
    "start": "react-native start",
    "test": "jest --watch --detectOpenHandles",
    "lint": "eslint .",
    "pod-install": "npx pod-install",
    "pod-update": "npx pod-update",
    "pods-ru": "cd ios && pod install --repo-update && cd ..",
    "link": "npx react-native link",
    "rename": "npx react-native-rename",
    "clean": "npx react-native-clean-project",
    "translate": "babel-node ./utils/googleTranslate.js",
    "set-splash": "react-native generate-bootsplash assets/splash/splash.png --background-color=FFFFFF --logo-width=300 --assets-path=assets/splash --flavor=main",
    "set-icon": "react-native set-icon --path ./assets/icon.png --background blue",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "build-web": "webpack --mode production",
    "start-web": "webpack-dev-server --config ./storybook/web/webpack.config.js --mode development",
    "start-storybook-web": "./node_modules/@storybook/react/bin/index.js",
    "build-storybook-web": "./node_modules/@storybook/react/bin/build.js",
    "storybook-web": "yarn run start-storybook-web"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "i18n-js": "^3.8.0",
    "jotai": "^1.3.7",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "^4.1.3",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-bootsplash": "^3.2.6",
    "react-native-localize": "^2.1.4",
    "react-native-reanimated": "^2.3.0-beta.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-tailwindcss": "^1.1.11",
    "react-native-web": "^0.17.5",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "@bam.tech/react-native-make": "^3.0.3",
    "@google-cloud/translate": "^6.3.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react": "^6.3.9",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.2.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.2.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-dom": "^17.0.2",
    "react-native-codegen": "^0.0.7",
    "react-native-storybook-loader": "^2.0.4",
    "react-test-renderer": "17.0.2",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
